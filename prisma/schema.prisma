// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model News {
  id      Int    @id @default(autoincrement())
  title   String
  content String
}

model UserInfo {
  userId   String  @id
  username String
  email    String?
  role     Int     @default(0)
}
